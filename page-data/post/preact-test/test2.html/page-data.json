{"componentChunkName":"component---src-templates-post-tsx","path":"/post/preact-test/test2.html","result":{"data":{"file":{"ext":".html","name":"test2","fields":{"content":"<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n  <meta charset=\"UTF-8\">\n  <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n  <title>Document</title>\n  <style>\n    * {\n      padding: 0;\n      margin: 0;\n      box-sizing: border-box;\n    }\n\n    #app>div>button {\n      background: linear-gradient(90deg,\n          rgba(2, 0, 36, 1) 0%,\n          rgba(9, 9, 121, 1) 35%,\n          rgba(0, 212, 255, 1) 100%);\n      color: white;\n    }\n  </style>\n</head>\n\n<body>\n  <div id=\"app\">\n    <button id=\"updateBtn\">update</button>\n    <div id=\"list\">\n    </div>\n  </div>\n\n  <script>\n    const $list = document.querySelector('#list');\n\n    function update() {\n      $list.innerHTML = '';\n\n      for (let i = 0; i < 100; i++) {\n        const $button = document.createElement('button');\n        $button.innerText = `button${i}`;\n        $button.style.fontSize = `${Math.floor(Math.random() * 40) + 10}px`;\n\n        $list.appendChild($button);\n      }\n    }\n\n    update();\n\n    document.querySelector('#updateBtn').addEventListener('click', async () => {\n      /*\n      await sleep(1000);\n      update();\n      await sleep(1000);\n      update();\n      await sleep(1000);\n      */\n      update();\n    });\n\n    function sleep(ms) {\n      return new Promise(resolve => setTimeout(() => resolve(), ms));\n    }\n  </script>\n</body>\n\n</html>","stampObject":{"modified":1667233157,"created":1667233157}}},"markdownRemark":null},"pageContext":{"slug":"preact-test/test2.html","slugNoExt":"preact-test/test2","repoName":"js-laboratory","userName":"jeong57281","profileUrl":"https://avatars.githubusercontent.com/u/44913775?v=4"}},"staticQueryHashes":[],"slicesMap":{}}